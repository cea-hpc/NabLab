Application Glace2d;

MainModule Glace2d glace2d
{
	nodeCoord = X;
	time = t;
	timeStep = delta_t;
	iterationMax = maxIterations;
	timeMax = stopTime;
}

VtkOutput
{
	periodReferenceVariable = glace2d.n;
	outputVariables = glace2d.rho as "Density";
}

